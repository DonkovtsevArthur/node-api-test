    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "D": "^1.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
